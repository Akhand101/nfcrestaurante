<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/FrameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#BFEFFF" >

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="10dp"
        android:layout_marginLeft="50dp"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp" >

            <ImageView
                android:id="@+id/reloj"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:paddingTop="5dp"
                android:src="@drawable/reloj" />

            <TextView
                android:id="@+id/horaHijoHistorico"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginLeft="5dp"
                android:text="Hora"
                android:textColor="@color/negro"
                android:textSize="18dp" />

        </LinearLayout>

        <FrameLayout
            android:id="@+id/FrameLayout1"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="2dp"
            android:layout_marginTop="3dp" >

            <ImageView
                android:id="@+id/imageBeam"
                android:layout_width="30dp"
                android:layout_height="30dp"
                android:src="@drawable/camarero" />

            <TextView
                android:id="@+id/camareroHijoHistorico"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_vertical"
                android:layout_marginLeft="33dp"
                android:gravity="right"
                android:text="Camarero"
                android:textColor="@color/negro"
                android:textSize="18sp" />

            <TextView
                android:id="@+id/precioHijoHistorico"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="right"
                android:layout_marginRight="10dp"
                android:text="Precio"
                android:textColor="@color/negro"
                android:textSize="18sp" />

        </FrameLayout>
    </LinearLayout>

</FrameLayout>